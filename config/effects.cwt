    ## cardinality = 0..1
    size = single_alias_right[formula]
    ## cardinality = 0..1
    size = <script_value>
    ## cardinality = 0..1
    size = <static_value>

    ## cardinality = 0..1
    type = scope[pop_type]

    ## cardinality = 0..1
    location = scope[location]

    fraction = value_field
    fraction = single_alias_right[formula]
    fraction = <script_value>
    fraction = <static_value>

