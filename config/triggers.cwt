alias[trigger:heir_score_country] = {
    target = scope[character]

    ## cardinality = 0..inf
    value == value_field
    ## cardinality = 0..inf
    value == <script_value>
    ## cardinality = 0..inf
    value == <static_value>
    ## cardinality = 0..inf
    value == single_alias_right[formula]

    ## cardinality = 0..inf
    alias_name[arithmetic_operation] = alias_match_left[arithmetic_operation]
}

