### Get the hypothetical heir score of the character for the target country, even if the character in question could not be an heir.
## scopes = { any }
alias[trigger:heir_score] = {
    target = scope[country]

    ## cardinality = 0..inf
    value == value_field
    ## cardinality = 0..inf
    value == <script_value>
    ## cardinality = 0..inf
    value == <static_value>
    ## cardinality = 0..inf
    value == single_alias_right[formula]

    ## cardinality = 0..inf
    alias_name[arithmetic_operation] = alias_match_left[arithmetic_operation]
}

