### Adds the current scope to an arbitrarily-named list (or creates the list if not already present) to be referenced later in the same trigger
### add_to_temporary_list = <name_of_list>
### add_to_temporary_list = { name = <name_of_list> value = <script_value> }
### NOTE, if adding a temporary target to a permanent list, the list will stay permanent
## scopes = { any }
alias[trigger:add_to_temporary_list] = value_set[temporary_list]
### Adds the current scope to an arbitrarily-named list (or creates the list if not already present) to be referenced later in the same trigger
### add_to_temporary_list = <name_of_list>
### add_to_temporary_list = { name = <name_of_list> value = <script_value> }
### NOTE, if adding a temporary target to a permanent list, the list will stay permanent
## scopes = { any }
alias[trigger:add_to_temporary_list] = {
    name = value_set[temporary_list]

    ## cardinality = 0..1
    target = scope_field
    ## cardinality = 0..1
    target = value_field
    ## cardinality = 0..1
    target = <script_value>
    ## cardinality = 0..1
    target = <static_value>
    ## cardinality = 0..1
    target = single_alias_right[formula]

    ## cardinality = 0..inf
    alias_name[duration] = alias_match_left[duration]
}

